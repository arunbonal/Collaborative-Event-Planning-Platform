<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Feedback List</title>
</head>
<body>
    <h1>Feedback List</h1>
    <a href="/feedback/new">Add Feedback</a>
    <table border="1" cellspacing="0" cellpadding="5">
        <thead>
            <tr>
                <th>Owner</th>
                <th>Event</th>
                <th>Rating</th>
                <th>Comments</th>
                <th>Date</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            <% feedbacks.forEach(feedback => { %>
                <tr>
                    <td><%= feedback.owner_name %></td>
                    <td><%= feedback.event_title %></td>
                    <td><%= feedback.rating %></td>
                    <td><%= feedback.comments %></td>
                    <td><%= feedback.date %></td>
                    <td>
                        <a href="/feedback/<%= feedback.feedback_id %>/edit">Edit</a>
                        <form action="/feedback/<%= feedback.feedback_id %>?_method=DELETE" method="POST" style="display:inline;">
                            <button type="submit">Delete</button>
                        </form>
                    </td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
